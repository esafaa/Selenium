<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNGListerSuite">
    <listeners>
        <listener class-name="listeners.Listeners"></listener>
    </listeners>
    <test name="loginTest">
        <classes>
            <class name="listeners.LoginTest">
                <methods>
                    <include name="loginByEmail"/>

                    <include name="loginByFacebook" />
                </methods>
            </class>
        </classes>
    </test>
</suite>